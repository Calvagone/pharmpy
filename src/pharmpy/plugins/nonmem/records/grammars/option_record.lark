root : _begin _options? _end

_options: (_options _sep)? option

_begin: _sep?
_end: ws? (COMMENT? newline)? COMMENT?
_sep: ws | ws? COMMENT? newline

newline: (newline COMMENT?)? NEWLINE ws?
ws: WS

option: KEY ("=" VALUE)?

KEY: ATOM
VALUE: ATOM
ATOM: /\([^)]*\)|[^\s=;]+/

%import .definitions (COMMENT, WS, NEWLINE)

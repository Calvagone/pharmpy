WS: /[ \x00\t]+/
WS_ALL: /\s+/

LINEENDING: (COMMENT? NEWLINE)+
NEWLINE: /\r?\n[ \x00\t]*/
COMMENT: /;[^\r\n]*/

DIGIT: "0".."9"
INT: DIGIT+
DECIMAL: (INT "." [INT] | "." INT)
SIGNED_INT: ["+" | "-"] INT

EXP: ("E" | "e") SIGNED_INT
FLOAT: (INT EXP | DECIMAL [EXP])
SIGNED_FLOAT: ["+" | "-"] FLOAT

NUMBER: (FLOAT | INT)
SIGNED_NUMBER: ["+" | "-"] NUMBER
NUMERIC: (NUMBER | SIGNED_NUMBER)

SYMBOL: /[_A-Za-z]\w+/

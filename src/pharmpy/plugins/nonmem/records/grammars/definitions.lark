WS_ALL: /\s+/
LINEENDING: (COMMENT? NEWLINE WS?)+

WS: /[ \x00\t]+/
NEWLINE: /\r?\n/
COMMENT: /;[^\r\n]*/

DIGIT: "0".."9"
INT: DIGIT+
DECIMAL: (INT "." [INT] | "." INT)
SIGNED_INT: ["+" | "-"] INT

EXP: ("E" | "e") SIGNED_INT
FLOAT: (INT EXP | DECIMAL [EXP])
SIGNED_FLOAT: ["+" | "-"] FLOAT

NUMBER: (FLOAT | INT)
SIGNED_NUMBER: ["+" | "-"] NUMBER
NUMERIC: (NUMBER | SIGNED_NUMBER)

SYMBOL: /[_A-Za-z]\w+/
